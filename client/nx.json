{
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "npmScope": "pg",
    "affected": {
        "defaultBase": "main"
    },
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/nx-cloud",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "generators": {
        "@nrwl/react": {
            "application": {
                "style": "none",
                "linter": "eslint",
                "babel": true
            },
            "component": {
                "style": "none"
            },
            "library": {
                "style": "none",
                "linter": "eslint"
            }
        }
    },
    "defaultProject": "wheel"
}
